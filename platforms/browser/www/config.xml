<?xml version='1.0' encoding='utf-8'?>
<widget id="org.arnastofnun.malid" version="0.1.02" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Málið</name>
    <description>
        Smáforrit fyrir Málið.is
    </description>
    <author email="ut@arnastofnun.is" href="http://arnastofnun.is">
        Stofnun Árna Magnússonar í íslenskum fræðum
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-targetSdkVersion" value="26" />
</widget>
